include nav-item

mixin nav(company)
  nav.fixed-top.navbar.navbar-dark.navbar-expand-md.bg-dark.pl-0
    .navbar-brand.px-0(href='#')
      .px-1.d-inline-block.align-top(src=`https://cdn2.rebbl.net/images/${logo}` height='30' alt='' class="" + cls role='button' data-toggle='dropdown' style="cursor: pointer;")
        img(src=`https://cdn2.rebbl.net/images/${logo}` height='30'  )
    button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarSupportedContent' aria-controls='navbarSupportedContent' aria-expanded='false' aria-label='Toggle navigation')
      span.navbar-toggler-icon
    #navbarSupportedContent.collapse.navbar-collapse.pl-1
      ul.navbar-nav.mr-auto
        li.nav-item.dropdown.mr-3(style="cursor: pointer;")
          a#navbarDropdown.nav-link.text-light.dropdown-toggle(alt='' role='button' data-toggle='dropdown') Sites
          div
            .dropdown-menu.bg-dark(aria-labelledby='navbarDropdown5')
              a.px-1.dropdown-item(href="/")
                img.col-2.p-0.d-md-inline-block.align-top.d-none(src='https://cdn2.rebbl.net/images/skills/CardSet.png')
                .col.d-inline-block.text-light Frontpage
              a.px-1.dropdown-item(href="/rebbl")
                img.col-2.p-0.d-md-inline-block.align-top.d-none(src='https://cdn2.rebbl.net/images/rebbl.png')
                .col.d-inline-block.text-light ReBBL
              a.px-1.dropdown-item(href="/rebbrl")
                img.col-2.p-0.d-md-inline-block.align-top.d-none(src='https://cdn2.rebbl.net/images/rebbrl.png')
                .col.d-inline-block.text-light ReBBRL
              a.px-1.dropdown-item(href="/clan")
                img.col-2.p-0.d-md-inline-block.align-top.d-none(src='https://cdn2.rebbl.net/images/clan.png')
                .col.d-inline-block.text-light Clan
              a.px-1.dropdown-item(href="https://imperium.rebbl.net" target="_blank")
                img.col-2.p-0.d-md-inline-block.align-top.d-none(src='https://cdn2.rebbl.net/images/imperium.png' style="max-width:20px")
                .col.d-inline-block.text-light Imperium
              a.px-1.dropdown-item(href="https://bounties.rebbl.net" target="_blank")
                img.col-2.p-0.d-md-inline-block.align-top.d-none(src='https://cdn2.rebbl.net/images/donate.png' style="max-width:20px")
                .col.d-inline-block.text-light Bounty Board
        case company
          when "rebbl"
              li.nav-item.dropdown.mr-3(style="cursor: pointer;")
                a#navbarDropdown2.nav-link.text-light.dropdown-toggle(alt='' role='button' data-toggle='dropdown') Main
                div
                  .dropdown-menu.bg-dark(aria-labelledby='navbarDropdown2')
                    a.dropdown-item.d-inline-block.text-light(href="/rebbl/standings/REBBL - Big O" ) Big O
                    a.dropdown-item.d-inline-block.text-light(href="/rebbl/standings/REBBL - GMan") GMan
                    a.dropdown-item.d-inline-block.text-light(href="/rebbl/standings/REBBL - REL" ) Rel
                    .dropdown-divider
                    a.dropdown-item.d-inline-block.text-light(href="/rebbl/rampup") Rampup
                    a.dropdown-item.d-inline-block.text-light(href="/rebbl/Stunty") Stunty
              li.nav-item.dropdown.mr-3(style="cursor: pointer;")
                a#navbarDropdown3.nav-link.text-light.dropdown-toggle(alt='' role='button' data-toggle='dropdown')
                  | Side leagues
                div
                  .dropdown-menu.bg-dark(aria-labelledby='navbarDropdown3')
                    a.dropdown-item.d-inline-block.text-light(href="/rebbl/standings/REBBLL%20" ) Beer
                    a.dropdown-item.d-inline-block.text-light(href="/rebbl/XScessively Elfly League") Elfly
              li.nav-item.dropdown.mr-3(style="cursor: pointer;")
                a#navbarDropdown4.nav-link.text-light.dropdown-toggle(alt='' role='button' data-toggle='dropdown')
                  | Offseason
                div
                  .dropdown-menu.bg-dark(aria-labelledby='navbarDropdown4')
                    a.dropdown-item.d-inline-block.text-light(href="/rebbl/playoffs/REBBL Playoffs Season 14") Playoffs
                    a.dropdown-item.d-inline-block.text-light(href="/rebbl/playoffs/Challenger's Cup XIV" ) Challenger Cup
                    .dropdown-divider
                    a.dropdown-item.d-inline-block.text-light(href="/rebbl/Greenhorn Cup") Greenhorn
              block navigation
          when "rebbrl"
              +nav-item("Minors", "/rebbrl/ReBBRL Minors League")
              +nav-item("College", "/rebbrl/ReBBRL College League")
              +nav-item("Upstarts", "/rebbrl/ReBBRL Upstarts")
              block navigation
          when "frontpage"
              +nav-item("Signups", "/signup/signups")
              +nav-item("Upcoming games", "/rebbl/upcoming")
              +nav-item("Ongoing games", "/rebbl/ongoing")
              //+nav-item("Summer Camp", "/rebbl/camping/summer")
              +nav-item("Star Players", "/bloodbowl/starplayers")
              +nav-item("Legendary players", "/bloodbowl/legendaryplayers")
              +nav-item("Villey's Guides", "/guides",true)
              block navigation
          when "clan"
              +nav-item("Clan", "/clan/clan")
              block navigation
          default
              block navigation
      ul.navbar-nav
        li.nav-item
          a.nav-link.text-light(href="/account") Account        

